import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.print("Enter the number : ");
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       
       int Count=0;
       while(n>0){
           int digit =n%10;
           Count++;
           n/=10;
       }
       System.out.print(Count);
    }
}
