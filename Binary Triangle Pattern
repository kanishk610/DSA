class Main {
    public static void main(String[] args) {
        for(int i=0;i<5;i++){
            int num=1;
            if(i%2==0){
               num=1;
            }else{
                num=0;
            }
          
            for(int j=0;j<=i;j++){
                System.out.print(num);
               num=1-num;
        
        }
        System.out.println();
        }
    }
}
