class Main {
    public static void main(String[] args) {
        for(int i=1;i<=4;i++){
            for(int h=1;h<=i;h++){
                System.out.print(h);
            }
            for(int j=0;j<2*(4-i);j++){
                System.out.print(" ");
            }
            for(int k=i;k>=1;k--){
                System.out.print(k);
            }
            System.out.println();
        }
    }
}
